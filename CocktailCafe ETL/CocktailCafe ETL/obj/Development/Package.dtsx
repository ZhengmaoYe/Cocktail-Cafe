                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
CocktailCafe.dbo.Sale_Receipt.ReceiptID AS Sales_DD,
CocktailCafe.dbo.Drink.Price AS DrinkPrice,
CocktailCafe.dbo.Drink.Cost AS DrinkCost,
