CocktailCafeDM.dbo.DimStore.StoreID_Sk,
CocktailCafeDM.dbo.DimDrink.DrinkID_SK AS DrinkID_SK,
CocktailCafeDM.dbo.DimCustomer.CustomerID_SK AS CustomerID_SK,
CocktailCafeDM.dbo.DimDate.Date_SK AS Date_SK

FROM CocktailCafe.dbo.Sale_Receipt
INNER JOIN CocktailCafe.dbo.Staff
ON CocktailCafe.dbo.Sale_Receipt.StaffID=CocktailCafe.dbo.Staff.StaffID
INNER JOIN CocktailCafe.dbo.Store_Location
ON CocktailCafe.dbo.Store_Location.LocationID=CocktailCafe.dbo.Staff.LocationID
INNER JOIN CocktailCafe.dbo.Drink_Receipt
ON CocktailCafe.dbo.Sale_Receipt.ReceiptID = CocktailCafe.dbo.Drink_Receipt.ReceiptID
INNER JOIN CocktailCafe.dbo.Drink
ON CocktailCafe.dbo.Drink_Receipt.DrinkID = CocktailCafe.dbo.Drink.DrinkID
INNER JOIN CocktailCafe.dbo.DrinkBase_Drink
ON CocktailCafe.dbo.DrinkBase_Drink.DrinkID=CocktailCafe.dbo.Drink.DrinkID
INNER JOIN CocktailCafe.dbo.Alcohol
ON CocktailCafe.dbo.Alcohol.AlcoholID = CocktailCafe.dbo.Drink.AlcoholID
INNER JOIN CocktailCafe.dbo.Coffee
ON CocktailCafe.dbo.Coffee.CoffeeID = CocktailCafe.dbo.Drink.CoffeeID
INNER JOIN CocktailCafe.dbo.OrderAmount
ON CocktailCafe.dbo.OrderAmount.DrinkID=CocktailCafe.dbo.Drink.DrinkID
INNER JOIN CocktailCafe.dbo.Customer
ON CocktailCafe.dbo.Customer.CustomerID = CocktailCafe.dbo.Sale_Receipt.CustomerID
INNER JOIN CocktailCafeDM.dbo.DimCustomer
ON CocktailCafe.dbo.Customer.CustomerID = CocktailCafeDM.dbo.DimCustomer.CustomerID_AK
INNER JOIN CocktailCafeDM.dbo.DimDate
ON CocktailCafeDM.dbo.DimDate.Date = CocktailCafe.dbo.Sale_Receipt.Date
INNER JOIN CocktailCafeDM.dbo.DimStore
ON CocktailCafeDM.dbo.DimStore.StoreID_AK = CocktailCafe.dbo.Store_Location.LocationID
INNER JOIN CocktailCafeDM.dbo.DimDrink
ON CocktailCafe.dbo.DrinkBase_Drink.Drinkbase_DrinkID=CocktailCafeDM.dbo.DimDrink.DrinkID_AK</property>
